{
  "name": "tymlez-guardian",
  "version": "1.0.0",
  "scripts": {
    "install": "lerna clean --no-private --yes && npm run bootstrap",
    "bootstrap": "./tools/bin/bootstrap.sh",
    "clean": "lerna run clean --stream && lerna clean --no-private --yes",
    "lint": "tsc --noEmit",
    "dev:docker": "docker-compose up -d --build",
    "dev": "docker-compose up -d mongo && lerna run watch --parallel",
    "deploy:manual": "npm run load-env && npm run tools deploy",
    "tools": "node -r esbuild-register ./tools/bin/index.ts",
    "load-env": "node -r esbuild-register ./tools/bin/index.ts load-env"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tymlez/guardian.git"
  },
  "keywords": [],
  "author": "",
  "bugs": {
    "url": "https://github.com/Tymlez/guardian/issues"
  },
  "homepage": "https://github.com/Tymlez/guardian#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.176",
    "@types/node": "^14.17.32",
    "@types/yargs": "^17.0.5",
    "aws-sdk": "^2.1021.0",
    "esbuild": "^0.13.12",
    "esbuild-register": "^3.0.0",
    "exec-sh": "^0.4.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "typescript": "^4.4.4",
    "yargs": "^17.2.1"
  }
}
